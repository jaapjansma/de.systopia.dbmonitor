<?xml version="1.0"?>
<extension key="de.systopia.dbmonitor" type="module">
  <file>dbmonitor</file>
  <name>Database Monitor</name>
  <description>Will try to spot conspicuous or "stuck" database queries.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>B. Endres</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.dbmonitor</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.dbmonitor</url>
    <url desc="Support">https://github.com/systopia/de.systopia.dbmonitor/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate></releaseDate>
  <version>0.3-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Dbmonitor</namespace>
  </civix>
</extension>
